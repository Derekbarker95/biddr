<h1> New Auction </h1>

<%= form_for @auction do |f| %>

  <%= f.label :title %>
  <%= f.text_field :title %>
  <br>
  <%= f.label :description %>
  <%= f.text_area :description %>
  <br>
  <%= f.label :reserve %>
  <%= f.number_field :reserve %>
  <br>
  <%= f.label :end_date %>
  <%= f.date_field :end_date %>
  <br>    
  <br>
  
  <%= f.submit %>
  
  <%end%>
